{
    "layout": {
        "type": "layout",
        "pages": [
            "panel-options",
            "panel-location",
            "panel-shortcuts"
        ],
        "panel-options": {
            "type": "page",
            "title": "Settings",
            "sections": [
                "opt-section",
                "day-section",
                "night-section"
            ]
        },
        "panel-location": {
            "type": "page",
            "title": "Location",
            "sections": [
                "location-section"
            ]
        },
        "panel-shortcuts": {
            "type": "page",
            "title": "Shortcuts",
            "sections": [
                "keybind-section",
                "info-section"
            ]
        },
        "info-section": {
            "type": "section",
            "title": "Tips",
            "keys": [
                "textTips",
                "textHelp",
                "textInfo"
            ]
        },
        "opt-section": {
            "type": "section",
            "title": "Options",
            "keys": [
                "enabled",
                "autoUpdate",
                "autoUpdateInterval",
                "adjustmentMethod",
                "labelScrollAction",
                "iconLabel",
                "symbolicIcon"
            ]
        },
        "day-section": {
            "type": "section",
            "title": "Day Settings",
            "keys": [
                "dayTemp",
                "dayBrightness",
                "gammaMix"
            ]
        },
        "night-section": {
            "type": "section",
            "title": "Night Settings",
            "keys": [
                "enabledNight",
                "nightTemp",
                "nightBrightness"
            ]
        },
        "keybind-section": {
            "type": "section",
            "title": "Key Bindings",
            "keys": [
                "keyToggle",
                "keyBrightnessUp",
                "keyBrightnessDown",
                "keyTempUp",
                "keyTempDown"
            ]
        },
        "location-section": {
            "type": "section",
            "title": "Location Settings",
            "keys": [
                "locationRemote",
                "locationUpdate",
                "locationLatitude",
                "locationLongitude",
                "aboutLocation"
            ]
        }
    },
    "enabled": {
        "type": "checkbox",
        "default": false,
        "description": "Enabled",
        "value": true
    },
    "autoUpdate": {
        "type": "checkbox",
        "default": true,
        "description": "Auto Update",
        "tooltip": "Continuously adjust color temperature",
        "value": true
    },
    "autoUpdateInterval": {
        "type": "spinbutton",
        "dependency": "autoUpdate=true",
        "default": 30,
        "min": 5,
        "max": 1800,
        "units": "Sec.",
        "step": 1,
        "description": "Update interval",
        "value": 30
    },
    "adjustmentMethod": {
        "type": "combobox",
        "default": "randr",
        "description": "Adjustment Method",
        "options": {
            "drm": "drm",
            "randr": "randr",
            "vidmode": "vidmode",
            "dummy": "dummy"
        },
        "tooltip": "Method to use to set color temperature",
        "value": "randr"
    },
    "labelScrollAction": {
        "type": "combobox",
        "default": "disabled",
        "description": "Icon Scroll Action",
        "options": {
            "Disabled": "disabled",
            "On/Off": "on_off",
            "Temperature": "temp",
            "Bright": "bright",
            "Gamma": "gamma"
        },
        "tooltip": "Action when scroll on applet icon.",
        "value": "disabled"
    },
    "iconLabel": {
        "type": "checkbox",
        "default": true,
        "description": "Show Label",
        "value": true
    },
    "symbolicIcon": {
        "type": "checkbox",
        "default": false,
        "description": "Use symbolic icon",
        "value": false
    },
    "keyToggle": {
        "type": "keybinding",
        "description": "On/Off",
        "default": "<Control>End",
        "value": "<Control>End"
    },
    "keyBrightnessUp": {
        "type": "keybinding",
        "description": "Increase brightness level",
        "default": "<Control>Page_Up",
        "value": "<Control>Page_Up"
    },
    "keyBrightnessDown": {
        "type": "keybinding",
        "description": "Decrease brightness level",
        "default": "<Control>Page_Down",
        "value": "<Control>Page_Down"
    },
    "keyTempUp": {
        "type": "keybinding",
        "description": "Increase temperature level",
        "default": "<Control><Shift>Page_Up",
        "value": "<Control><Shift>Page_Up"
    },
    "keyTempDown": {
        "type": "keybinding",
        "description": "Decrease temperature level",
        "default": "<Control><Shift>Page_Down",
        "value": "<Control><Shift>Page_Down"
    },
    "dayTemp": {
        "type": "spinbutton",
        "default": 5900,
        "min": 1000,
        "max": 9000,
        "units": "K",
        "step": 1,
        "description": "Temperature",
        "value": 5900
    },
    "dayBrightness": {
        "type": "spinbutton",
        "default": 100,
        "min": 10,
        "max": 100,
        "units": "%",
        "step": 1,
        "description": "Brightness",
        "value": 100
    },
    "gammaMix": {
        "type": "spinbutton",
        "default": 1,
        "min": 0.5,
        "max": 5,
        "units": "",
        "step": 0.01,
        "description": "Gamma",
        "value": 1
    },
    "enabledNight": {
        "type": "checkbox",
        "default": false,
        "description": "Night Enabled",
        "value": true
    },
    "nightTemp": {
        "type": "spinbutton",
        "dependency": "enabledNight=true",
        "default": 3500,
        "min": 1000,
        "max": 9000,
        "units": "K",
        "step": 1,
        "description": "Temperature",
        "value": 3500
    },
    "nightBrightness": {
        "type": "spinbutton",
        "dependency": "enabledNight=true",
        "default": 100,
        "min": 10,
        "max": 100,
        "units": "%",
        "step": 1,
        "description": "Brightness",
        "value": 100
    },
    "locationRemote": {
        "type": "checkbox",
        "default": true,
        "description": "Remote Location",
        "tooltip": "Native geoclue on redshift can make the experience slow.",
        "value": false
    },
    "locationUpdate": {
        "type": "button",
        "description": "Update Location",
        "dependency": "locationRemote=true",
        "callback": "setLocation",
        "tooltip": "Native geoclue on redshift can make the experience slow."
    },
    "locationLatitude": {
        "type": "entry",
        "default": "0",
        "description": "Latitude",
        "value": "42.9685"
    },
    "locationLongitude": {
        "type": "entry",
        "default": "0",
        "description": "Longitude",
        "value": "-78.7492"
    },
    "aboutLocation": {
        "type": "label",
        "description": "Native geoclue on redshift can make the experience slow.\nUsing: https://geolocation-db.com/"
    },
    "textTips": {
        "type": "label",
        "description": "* Right-click the sliders to reset the default (neutral) value."
    },
    "textHelp": {
        "type": "label",
        "description": "When update to a new version make sure you reset the configs to default by deleting the ~/.cinnamon/configs/qredshift@quintao/qredshift@quintao.json.\n You will lost all your configs."
    },
    "textInfo": {
        "type": "label",
        "description": "If you like and use this applet take the time to visit it's page on Cinnamon Spices and give it a star. You only need a github account to do this."
    },
    "__md5__": "c8f429dce95ef978dce1531ae6501245"
}